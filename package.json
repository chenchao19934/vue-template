{
  "name": "zw-vue-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "cross-env NODE_ENV=production vite build && esno ./build/script/postBuild.ts",
    "build:test": "cross-env vite build --mode test && esno ./build/script/postBuild.ts",
    "preview": "vite preview",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "lint:eslint": "eslint --cache \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write .",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "prepare": "husky install",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "reinstall": "rimraf pnpm-lock.yaml && rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && npm install"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "@iconify/iconify": "^2.1.2",
    "@vue/runtime-core": "^3.2.29",
    "@vue/shared": "^3.2.29",
    "@vueuse/core": "^7.5.4",
    "ant-design-vue": "^3.0.0-alpha.14",
    "axios": "^0.25.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^14.2.0",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.9",
    "qs": "^6.10.3",
    "vue": "^3.2.25",
    "vue-i18n": "^9.2.0-beta.26",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/crypto-js": "^4.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash-es": "^4.17.5",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitejs/plugin-legacy": "^1.6.4",
    "@vitejs/plugin-vue": "^2.0.1",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.2.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "esno": "^0.14.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.2.0",
    "postcss": "^8.4.5",
    "postcss-html": "^1.3.0",
    "postcss-scss": "^4.0.3",
    "prettier": "2.5.1",
    "rimraf": "^3.0.2",
    "sass": "^1.49.0",
    "sass-loader": "^12.4.0",
    "stylelint": "^14.2.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0",
    "typescript": "^4.5.4",
    "unplugin-vue-components": "^0.17.13",
    "vite": "^2.7.13",
    "vite-plugin-compression": "^0.4.0",
    "vite-plugin-html": "^2.1.2",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-optimize-persist": "^0.1.2",
    "vite-plugin-package-config": "^0.1.1",
    "vite-plugin-style-import": "^1.4.1",
    "vite-plugin-svg-icons": "^1.1.0",
    "vite-plugin-windicss": "^1.6.3",
    "vue-eslint-parser": "^8.0.1",
    "vue-tsc": "^0.30.6",
    "windicss": "^3.4.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "repository": {
    "type": "git",
    "url": "git+http://192.168.3.36:800/root/zw-vue-template.git"
  },
  "author": "ZW",
  "lint-staged": {
    "*.{vue,ts,tsx}": "eslint --cache --fix",
    "**/*": "prettier --write --ignore-unknown",
    "*.{vue,less,postcss,css,scss}": "stylelint --cache --fix"
  },
  "vite": {
    "optimizeDeps": {
      "include": [
        "@ant-design/icons-vue",
        "ant-design-vue",
        "ant-design-vue/es",
        "ant-design-vue/es/breadcrumb/style",
        "ant-design-vue/es/button/style",
        "ant-design-vue/es/col/style",
        "ant-design-vue/es/config-provider/style",
        "ant-design-vue/es/descriptions/style",
        "ant-design-vue/es/divider/style",
        "ant-design-vue/es/drawer/style",
        "ant-design-vue/es/dropdown/style",
        "ant-design-vue/es/form/style",
        "ant-design-vue/es/input-number/style",
        "ant-design-vue/es/input/style",
        "ant-design-vue/es/layout/style",
        "ant-design-vue/es/locale/zh_CN",
        "ant-design-vue/es/menu/style",
        "ant-design-vue/es/modal/style",
        "ant-design-vue/es/pagination/style",
        "ant-design-vue/es/popconfirm/style",
        "ant-design-vue/es/popover/style",
        "ant-design-vue/es/radio/style",
        "ant-design-vue/es/row/style",
        "ant-design-vue/es/select/style",
        "ant-design-vue/es/spin/style",
        "ant-design-vue/es/table/style",
        "ant-design-vue/es/tag/style",
        "ant-design-vue/es/tooltip/style",
        "ant-design-vue/es/tree/style",
        "axios",
        "crypto-js/aes",
        "crypto-js/enc-base64",
        "crypto-js/enc-utf8",
        "crypto-js/md5",
        "crypto-js/mode-ecb",
        "crypto-js/pad-pkcs7",
        "crypto-js/sha1",
        "lodash",
        "lodash-es",
        "nprogress",
        "pinia",
        "qs",
        "vue",
        "vue-i18n",
        "vue-router"
      ]
    }
  }
}
